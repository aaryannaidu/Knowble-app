{
  "expo": {
    "name": "Knowble-app",
    "slug": "Knowble-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "owner": "aaryannaidu",
    "icon": "./assets/images/icon.png",
    "scheme": "knowbleapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "bundleIdentifier": "com.knowble.app.ios",
      "supportsTablet": true
    },
    "android": {
      "package": "com.knowble.app.android",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font",
      "expo-web-browser",
      "expo-secure-store",
      [
        "@react-native-google-signin/google-signin",
        { "iosUrlScheme": "com.googleusercontent.apps.238570610028-amf9iue9t4mqihsbras06r0drdlouqgv" }
      ],
      "expo-video"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "528a24c6-431d-477b-a283-264e9a6c3f98"
      }
    }
  }
}
